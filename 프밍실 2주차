//워밍업5 65000 저장 자료형 중 메모리 가장 소모 적은것은? (signed short, unsigned short, long, int)-> unsigned short
//워밍업6 다음 실행 결과 예측
#include <stdio.h>
int main(void) {
 int x;
 float y;
 y=x=2.5; //C에서 대입연산은 오른쪽에서 왼쪽으로 진행. x=2.5 => x=2(int x) => y=2 => y=2.0
 printf("x=%d y=%f \n", x, y);
 return 0;
}
// 워밍업7 정수 18 0001 0010 아님????

//객5 출력 5 9인데 왜 틀렸담,,
#include <stdio.h>
int main() {
 int i, sum=0;
 for (i=1; i<=5; i++) {
  if (i%2==0)
    continue;
  sum+=i;
  if (sum>5)
    break;
 }
 printf("%d %d", i, sum);
 return 0;
}

//객11 출력 3 6 (do-while은 일단 실행 후 반복문 여부 결정이므로 n=3, sum=3!=6)
#include <stdio.h>
int main() {
 int n=0, sum=0;
 do {
    sum+= ++n;
 } while (n<3 && sum<5);
 printf("%d %d", n, sum);
 return 0;
}

//객14 https://unleashed-player-670.notion.site/14-288a93cf956b80cca14fd7befd1d3b7e
#include <stdio.h> 
int main() {
 int x=5;
 while (x>0) {
   if (x%2==0) {
       x-=2;
       continue;
   }
   x--;
   if (x==2)
       break;
 }
 printf("%d", x);
 return 0;
} 
